{
    "manifest_version": 3,
    "name": "Download Github Files",
    "description" : "Allows one to download a file from Github by double click when enabled",
    "version" : "1.1",
    "icons": {
        "16": "images/github-16.png",
        "32": "images/github-32.png",
        "128": "images/github-128.png"
      },
      "background" : {
        "service_worker" : "background.js"
      },
      "action": {
      "default_icon": {
        "16": "images/github-16.png",
        "32": "images/github-32.png",
        "128": "images/github-128.png"
      }
    },
    "permissions" : ["activeTab", "scripting","downloads","webNavigation"]
  }